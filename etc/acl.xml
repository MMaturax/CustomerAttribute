<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_attributes">
                        <resource id="Magenuts_CustomerAttribute::customer_attributes" title="Customer Attributes" sortOrder="50" />
                        <resource id="Magenuts_CustomerAttribute::save" title="Save Attribute" sortOrder="10" />
